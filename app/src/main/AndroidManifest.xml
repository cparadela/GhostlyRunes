<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.GhostlyRunes">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.GhostlyRunes.MainActivity"/>
        <activity android:name="com.GhostlyRunes.StarPatternActivity" />
        <activity android:name="com.GhostlyRunes.CompassActivity" />
        <activity android:name="com.GhostlyRunes.ZigZagPatternActivity" />
        <activity android:name="com.GhostlyRunes.GyroActivity" />
        <activity android:name="com.GhostlyRunes.MainMenu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>